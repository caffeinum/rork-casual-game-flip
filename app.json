{
  "expo": {
    "name": "Casual Game Flip",
    "slug": "casual-game-flip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.casual-game-flip",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.casual-game-flip"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "output": "server"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork-casual-game-flip.vercel.app/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://rork-casual-game-flip.vercel.app/"
      },
      "eas": {
        "projectId": "9ecffee8-084f-4291-b9ce-5da66fbc085a"
      }
    }
  }
}
